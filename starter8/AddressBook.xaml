<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="starter8.AddressBook"
             Title="Address Book">

    <StackLayout Padding="30">
        <Entry x:Name="names" Placeholder="Your Name" />
        <Entry x:Name="emails" Placeholder="Your Email" />
        <Entry x:Name="phones" Placeholder="Your Phone Contact " />
        <Button Text="Save" Clicked="OnLoginButtonClicked" />
    </StackLayout>
</ContentPage>
